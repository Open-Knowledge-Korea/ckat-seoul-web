<div id="title-panel">
        <h1>KCH</h1>
</div>
    
<div class="content-area">
	<table width="100%">
		<tr>	
		    <td id="left-panels" width="20%">
			<div ex:role="facet" ex:facetClass="TextSearch"></div>
				<div ex:role="facet" ex:expression=".type" ex:facetLabel="type"></div>
				<div ex:role="facet" ex:expression=".do" ex:facetLabel="do"></div>
				<div ex:role="facet" ex:expression=".si" ex:facetLabel="si"></div>
				<div ex:role="facet" ex:expression=".gun" ex:facetLabel="gun"></div>
				<div ex:role="facet" ex:expression=".dong" ex:facetLabel="gun"></div>
				<div ex:role="facet" ex:expression=".temporal" ex:facetLabel="temporal"></div>
		    </td>

			<td id="middle-column">
				<div ex:role="viewPanel" style="padding: 2em 0.5in;" valign="middle">
					<div ex:role="view"
					ex:viewClass="Thumbnail"
					ex:showAll="true"
					ex:orders=".id"
					ex:possibleOrders=".label">
						<div ex:role="lens" class="chThumbnail" style="display: none; margin: 0.5em; padding: 1.0em;">
							<div><span ex:content=".label"></span></div>
							<div class="line"></div>
							<div><span ex:content=".id"></span></div>							
						</div>
					</div>
				</div>

			</td>
			<td>
			<div id="popupContact"  style="overflow-y:scroll;width:600px;height:500px">
				<a id="popupContactClose">x</a>
				<h1 id="titleText"></h1>			
				<p id="contactArea"></p>
			</div>
			<div id="backgroundPopup"></div>
		    </div>
			</td>
		</tr>
	</table>	
